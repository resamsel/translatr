{
  "name": "translatr",
  "version": "3.0.1",
  "author": "Ren√© Panzar",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/resamsel/translatr/issues"
  },
  "homepage": "https://github.com/resamsel/translatr#readme",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"{apps,libs}/**/*.{ts,css,scss}\"",
    "prettier:affected": "pretty-quick --pattern='ui/{apps,libs}/**/*.{ts,css,scss}'",
    "prestart": "npm run buildinfo",
    "start": "ng serve --host 0.0.0.0 --port 4210",
    "start:admin": "ng serve translatr-admin --host 0.0.0.0 --port 4211",
    "start:docs": "ng serve docs --host 0.0.0.0 --port 4212",
    "generate": "dist/lets-generate",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "lint:fix": "./node_modules/.bin/nx workspace-lint --fix && ng lint --fix",
    "test": "ng test translatr --watch=false --code-coverage=true",
    "test:ci": "ng test --watch=false --code-coverage=true",
    "test:coverage": "ng test --watch=true --code-coverage=true",
    "test:ui:coverage:ci": "ng test translatr --watch=false --code-coverage=true",
    "e2e:ci": "ng e2e translatr-e2e --headless --prod",
    "e2e:ui:watch": "ng e2e translatr-e2e --watch",
    "coveralls": "cat ./coverage/*/*/lcov.info | coveralls",
    "prebuild:ci": "npm run buildinfo",
    "build:ci": "npm run lint && npm run test:ci && npm run e2e:ci && npm run build:prod",
    "prebuild:prod": "npm run buildinfo",
    "build:prod": "npm run build:ui:prod && npm run build:admin:prod",
    "build:ui:prod": "ng build translatr --prod --output-path ../public/ui",
    "build:admin:prod": "ng build translatr-admin --prod --output-path ../public/admin",
    "build:generate": "ng build cli-generate",
    "postbuild:generate": "tools/make-command.js dist/apps/cli-generate/main.js dist/lets-generate",
    "build:generate:prod": "ng build cli-generate --prod",
    "postbuild:generate:prod": "tools/make-command.js dist/apps/cli-generate/main.js dist/lets-generate",
    "buildinfo": "node tools/build-info.js",
    "postpull": "npm install",
    "precommit": "npm run prettier:affected && npm run build:ci",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "kill:server": "fuser -k 9000/tcp 4200/tcp 4211/tcp 4211/tcp; echo 'ports freed'",
    "nx": "nx",
    "prebundle-report": "ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/apps/translatr/stats-es2015.json",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:pull": "npm run i18n:pull:backend && npm run i18n:pull:ui && npm run i18n:pull:admin",
    "i18n:pull:backend": "cd .. && translatr pull",
    "i18n:pull:ui": "cd apps/translatr && translatr pull",
    "i18n:push:ui": "cd apps/translatr && translatr push",
    "i18n:pull:admin": "cd apps/translatr-admin && translatr pull",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/localize": "9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@ctrl/ngx-codemirror": "2.1.1",
    "@momothepug/tsmodule-alias": "^1.0.0",
    "@ngneat/hotkeys": "1.0.1",
    "@ngneat/transloco": "2.17.2",
    "@ngneat/transloco-locale": "1.3.0",
    "@ngneat/transloco-messageformat": "1.2.1",
    "@ngneat/transloco-preload-langs": "2.0.2",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/entity": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@nrwl/angular": "8.12.11",
    "@oclif/command": "1.6.1",
    "@oclif/config": "1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "codemirror": "5.54.0",
    "core-js": "^2.6.11",
    "d3": "5.16.0",
    "d3-time": "^1.1.0",
    "jasmine-marbles": "^0.6.0",
    "messageformat": "2.3.0",
    "moment": "2.26.0",
    "ngx-gravatar": "^7.2.2",
    "ngx-moment": "^3.5.0",
    "random-name": "^0.1.2",
    "runes": "^0.4.3",
    "rxjs": "6.5.5",
    "tslib": "^1.10.0",
    "underscore": "1.10.2",
    "zone.js": "~0.10.2",
    "snyk": "^1.363.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@ngneat/spectator": "5.8.0",
    "@ngneat/transloco-keys-manager": "^1.0.0",
    "@ngneat/transloco-optimize": "1.0.1",
    "@ngneat/transloco-validator": "1.0.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@nrwl/cypress": "9.3.0",
    "@nrwl/jest": "8.12.11",
    "@nrwl/linter": "8.12.11",
    "@nrwl/node": "8.12.11",
    "@nrwl/schematics": "8.12.11",
    "@nrwl/workspace": "8.12.11",
    "@oclif/dev-cli": "^1.22.2",
    "@schuchard/prettier": "^3.0.0",
    "@types/d3": "^5.7.2",
    "@types/d3-time": "^1.0.10",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^23.3.14",
    "@types/jquery": "3.3.6",
    "@types/node": "^12.11.1",
    "@types/sanitize-html": "1.23.0",
    "@types/underscore": "1.10.0",
    "codelyzer": "^5.1.2",
    "coveralls": "3.1.0",
    "cypress": "4.8.0",
    "dateformat": "^3.0.3",
    "husky": "4.2.5",
    "jest": "^24.9.0",
    "jest-preset-angular": "7.1.1",
    "lint-staged": "9.5.0",
    "ngx-build-plus": "^9.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "rxjs-tslint-rules": "4.32.0",
    "ts-jest": "^24.3.0",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.11.0",
    "tslint-angular": "3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "3.7.0",
    "xmlhttprequest": "^1.8.0"
  },
  "files": [
    "/dist/lets-generate"
  ],
  "oclif": {
    "commands": "./apps/cli-generate/src/commands",
    "bin": "translatr",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "apps/translatr*/src/assets/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "snyk": true
}
