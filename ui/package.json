{
  "name": "translatr-ui",
  "version": "3.3.0-0",
  "author": "RenÃ© Panzar",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/resamsel/translatr/issues"
  },
  "homepage": "https://github.com/resamsel/translatr#readme",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"{apps,libs}/**/*.{ts,css,scss}\"",
    "prettier:affected": "pretty-quick --pattern='ui/{apps,libs}/**/*.{ts,css,scss}'",
    "prestart": "npm run buildinfo",
    "start": "ng serve --host 0.0.0.0 --port 4210",
    "start:admin": "ng serve translatr-admin --host 0.0.0.0 --port 4211",
    "start:docs": "ng serve docs --host 0.0.0.0 --port 4212",
    "start:lets-generate": "ng serve lets-generate",
    "lets-generate": "dist/lets-generate",
    "prereadme:lets-generate": "npm run build:lets-generate",
    "readme:lets-generate": "dist/lets-generate --document > ./libs/generator/README.md",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "lint:fix": "./node_modules/.bin/nx workspace-lint --fix && ng lint --fix",
    "test": "ng test --watch=false --code-coverage=true",
    "e2e": "ng e2e translatr-e2e --headless --prod",
    "e2e:ui:watch": "ng e2e translatr-e2e --watch",
    "coveralls": "cat ./coverage/*/*/lcov.info | coveralls",
    "prebuild": "npm run buildinfo",
    "build": "npm run lint && npm run test && npm run e2e && npm run build:prod",
    "prebuild:prod": "npm run buildinfo",
    "build:prod": "npm run build:ui:prod && npm run build:admin:prod",
    "build:ui:prod": "ng build translatr --prod --output-path ../public/ui",
    "build:admin:prod": "ng build translatr-admin --prod --output-path ../public/admin",
    "build:lets-generate": "ng build lets-generate",
    "postbuild:lets-generate": "tools/make-command.js dist/apps/lets-generate/main.js dist/lets-generate",
    "build:lets-generate:prod": "ng build lets-generate --prod",
    "postbuild:lets-generate:prod": "tools/make-command.js dist/apps/lets-generate/main.js dist/lets-generate",
    "prebuild:lets-generate:docker": "npm run build:lets-generate:prod",
    "build:lets-generate:docker": "docker build -t resamsel/translatr-loadgenerator:$npm_package_version .",
    "publish:lets-generate:docker": "docker push resamsel/translatr-loadgenerator:$npm_package_version",
    "buildinfo": "node tools/build-info.js",
    "postpull": "npm install",
    "precommit": "npm run prettier:affected && npm run build",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "nx": "nx",
    "prebundle-report": "ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/apps/translatr/stats-es2015.json",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:pull": "npm run i18n:pull:ui && npm run i18n:pull:admin",
    "i18n:pull:ui": "cd apps/translatr && translatr pull",
    "i18n:pull:admin": "cd apps/translatr-admin && translatr pull",
    "i18n:push": "npm run i18n:push:ui && npm run i18n:push:admin",
    "i18n:push:ui": "cd apps/translatr && translatr push",
    "i18n:push:admin": "cd apps/translatr-admin && translatr push",
    "snyk-protect": "snyk protect",
    "generate-icons": "svg-to-ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "11.0.5",
    "@angular/localize": "11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@ctrl/ngx-codemirror": "4.1.0",
    "@fortawesome/angular-fontawesome": "0.8.1",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@momothepug/tsmodule-alias": "^1.0.0",
    "@ngneat/hotkeys": "1.1.2",
    "@ngneat/svg-icon": "2.0.0",
    "@ngneat/transloco": "2.20.0",
    "@ngneat/transloco-locale": "1.4.0",
    "@ngneat/transloco-messageformat": "1.3.1",
    "@ngneat/transloco-preload-langs": "2.0.4",
    "@ngrx/effects": "10.1.2",
    "@ngrx/entity": "10.1.2",
    "@ngrx/router-store": "10.1.2",
    "@ngrx/store": "10.1.2",
    "@nrwl/angular": "8.12.11",
    "@oclif/command": "1.6.1",
    "@oclif/config": "1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "cli-ux": "5.5.0",
    "codemirror": "5.59.1",
    "core-js": "2.6.12",
    "d3": "5.16.0",
    "d3-time": "^1.1.0",
    "globby": "11.0.1",
    "jest-transform-stub": "2.0.0",
    "messageformat": "2.3.0",
    "moment": "2.26.0",
    "ngx-gravatar": "^7.2.2",
    "ngx-moment": "^3.5.0",
    "random-name": "^0.1.2",
    "runes": "^0.4.3",
    "rxjs": "6.5.5",
    "snyk": "1.437.3",
    "tslib": "^2.0.0",
    "underscore": "1.10.2",
    "xmlhttprequest": "1.8.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@ngneat/spectator": "5.8.0",
    "@ngneat/transloco-keys-manager": "^1.0.0",
    "@ngneat/transloco-optimize": "1.0.1",
    "@ngneat/transloco-validator": "1.0.0",
    "@ngrx/store-devtools": "10.1.2",
    "@nrwl/cypress": "9.3.0",
    "@nrwl/jest": "8.12.11",
    "@nrwl/linter": "8.12.11",
    "@nrwl/node": "11.0.20",
    "@nrwl/schematics": "8.12.11",
    "@nrwl/workspace": "8.12.11",
    "@oclif/dev-cli": "1.26.0",
    "@schuchard/prettier": "^3.0.0",
    "@types/d3": "5.16.4",
    "@types/d3-time": "1.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^23.3.14",
    "@types/jquery": "3.3.6",
    "@types/node": "12.12.62",
    "@types/sanitize-html": "1.23.0",
    "@types/underscore": "1.10.0",
    "codelyzer": "^6.0.0",
    "coveralls": "3.1.0",
    "cypress": "4.8.0",
    "dateformat": "3.0.3",
    "husky": "4.2.5",
    "jasmine-marbles": "^0.6.0",
    "jest": "^24.9.0",
    "jest-junit": "12.0.0",
    "jest-preset-angular": "7.1.1",
    "lint-staged": "9.5.0",
    "ngx-build-plus": "^9.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "rxjs-tslint-rules": "4.34.7",
    "svg-to-ts": "^5.0.0",
    "ts-jest": "^24.3.0",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.0",
    "tslint-angular": "3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.0.5",
    "webpack-bundle-analyzer": "3.7.0"
  },
  "files": [
    "/dist/lets-generate"
  ],
  "oclif": {
    "commands": "./apps/lets-generate/src/commands",
    "bin": "translatr",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "apps/translatr*/src/assets/i18n/*.json": [
      "transloco-validator"
    ]
  },
  "snyk": true,
  "svg-to-ts": {
    "generateType": "false",
    "delimiter": "KEBAB",
    "conversionType": "files",
    "iconsFolderName": "svg",
    "prefix": "app",
    "srcFiles": [
      "./assets/svg/*.svg"
    ],
    "outputDirectory": "./apps/translatr/src/assets",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  }
}
