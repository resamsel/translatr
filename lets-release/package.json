{
  "name": "lets-release",
  "description": "Release workflow for main braching workflow",
  "version": "0.1.0",
  "author": "RenÃ© Panzar @resamsel",
  "bin": {
    "lets-release": "./bin/run"
  },
  "bugs": "https://github.com/resamsel/translatr/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.0",
    "simple-git": "2.24.0",
    "tslib": "1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.25.1",
    "@types/jest": "26.0.16",
    "@types/node": "10.17.48",
    "eslint": "5.16.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.1.0",
    "eslint-config-prettier": "7.0.0",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/resamsel/translatr",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/cli.js",
  "oclif": {
    "bin": "lets-release"
  },
  "repository": "resamsel/translatr",
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "npm run lint -- --fix",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "jest",
    "posttest": "npm run lint",
    "version": "oclif-dev readme && git add README.md",
    "lets-release": "bin/run --dry-run"
  },
  "types": "lib/index.d.ts",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
